AddContent("Switch", "Auto Helper", [[
AutoHelper = true
]],[[
AutoHelper = false
]])


AddContent("Switch", "Auto Fill", [[
AutoFill = true
]],[[
AutoFill = false
]])

local TextBox = AddContent("TextBox","Auto Fill Speed")
TextBox.Text = 0
TextBox:GetPropertyChangedSignal("Text"):Connect(function()
AutoFillSpeed = TextBox.Text
end)



